{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "sky24-icas",
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/register",
        "destination": "/register.html"
      },
      {
        "source": "/rules",
        "destination": "/rules.html"
      },
      {
        "source": "/round/**",
        "destination": "/round/[id]/index.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/**",
        "destination": "/404.html"
      }
    ]
  }
}
